.status-active {
	background: #dff2de;
	border-radius: 50px;
	padding-left: 24px;
	position: relative;
	display: inline-block;
	padding: 4px 10px 4px 23px;
}

span.status-active::after {
	content: "";
	display: block;
	position: absolute;
	background: #2ea62e;
	width: 12px;
	height: 12px;
	border-radius: 100%;
	left: 8px;
	top: 8px;
}

.cust-box-shadow .form-inline i.fa.fa-angle-down {
