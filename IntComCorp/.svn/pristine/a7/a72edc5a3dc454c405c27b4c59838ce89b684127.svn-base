@import
	url('https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;1,100;1,300;1,400;1,500;1,600;1,800&display=swap')
	;

body {
	background: #eee;
	font-family: 'Montserrat', sans-serif;
	font-size: 14px;
}

@font-face {
	font-family: 'Material Icons';
	font-style: normal;
	font-weight: 400;
	src: url(https://example.com/MaterialIcons-Regular.eot);
	/* For IE6-8 */
	src: local('Material Icons'), local('MaterialIcons-Regular'),
		url(https://example.com/MaterialIcons-Regular.woff2) format('woff2'),
		url(https://example.com/MaterialIcons-Regular.woff) format('woff'),
		url(https://example.com/MaterialIcons-Regular.ttf) format('truetype');
}

.material-icons {
	font-family: 'Material Icons';
	font-weight: normal;
	font-style: normal;
	font-size: 24px; /* Preferred icon size */
	display: inline-block;
	line-height: 1;
	text-transform: none;
	letter-spacing: normal;
	word-wrap: normal;
	white-space: nowrap;
	direction: ltr;
	/* Support for all WebKit browsers. */
	-webkit-font-smoothing: antialiased;
	/* Support for Safari and Chrome. */
	text-rendering: optimizeLegibility;
	/* Support for Firefox. */
	-moz-osx-font-smoothing: grayscale;
	/* Support for IE. */
	font-feature-settings: 'liga';
}

.login-container {
	display: flex;
	background: url('../images/loginbg.png');
	height: 100vh;
	align-items: center;
	justify-content: center;
	font-family: 'Montserrat', sans-serif;
}

.form-container {
	width: 600px;
	background: #fff;
	padding: 40px;
	border-radius: 4px;
	box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.1);
}

.login-heading {
	font-size: 45px;
	font-weight: bold;
	color: #10497e;
	padding-top: 20px;
	padding-bottom: 10px
}

.login-container p, .login-container p a {
	color: #00274d;
}

.login-container .form-control.custom {
	border-radius: 0px;
	border-width: 0px 0px 1px 0px;
	padding: 0px;
	color: #000;
}

.login-container label {
	color: #717171;
}

.btn-login {
	background: linear-gradient(-90deg, #cc242c, #962328);
	border-radius: 30px;
	padding: 10px 20px;
	border: none;
	transition: all 0.3s ease-in-out;
	-webkit-transition: all 0.3s ease-in-out;
}

.btn-login:hover {
	background: linear-gradient(-90deg, #962328, #cc242c);
}

.forget-text {
	color: #10497e;
	font-weight: 500;
}

.social-links a {
	color: #1ca3db
}

.social-links a:hover {
	color: #007bff;
}

