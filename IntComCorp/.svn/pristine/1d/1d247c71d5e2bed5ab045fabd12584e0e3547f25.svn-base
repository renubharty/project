	<header>
		<nav class="navbar navbar-expand-lg">
			<a class="navbar-brand logo" th:href="@{/}"> <!-- <img
				src="images/logo.jpg" /> -->
			</a>
			<button class="navbar-toggler" type="button" data-toggle="collapse"
				data-target="#navbarSupportedContent"
				aria-controls="navbarSupportedContent" aria-expanded="false"
				aria-label="Toggle navigation">
				<span class="navbar-toggler-icon"><i class="fa fa-navicon"></i></span>
			</button>

			<div class="collapse navbar-collapse" id="navbarSupportedContent">
				<ul class="navbar-nav mr-auto">
					<li class="nav-item active"><a class="nav-link active"
						th:href="@{/monitoring}">Monitoring</a></li>
					<li class="nav-item"><a class="nav-link"
						th:href="@{/inventory}">Inventory</a></li>
					<li class="nav-item"><a class="nav-link" th:href="@{/reperts}">Reports</a>
					</li>
					<li class="nav-item"><a class="nav-link"
						th:href="@{/configuration}">Configuration</a></li>
					<li class="nav-item"><a class="nav-link"
						th:href="@{/administration}">Administration</a></li>
				</ul>

				<div class="form-inline my-2 my-lg-0">
					<div class="search-bar">
						<input class="form-control" type="search" placeholder="Search"
							aria-label="Search">
						<button class="btn" type="submit">
							<i class="fa fa-search"></i>
						</button>
					</div>



				</div>
			</div>
		</nav>
		<div class="main-nav">
			<nav class="navbar navbar-expand-lg">

				<button class="navbar-toggler" type="button" data-toggle="collapse"
