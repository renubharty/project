	<section class="wrapper">
		<div class="container-fluid">
			<div class="row">
				<div class="col-sm-12">
					<div class="main-head group-outer">
						<h1 class="">Hosts</h1>
						<div class="head-right">
							<div class="group-box">
								<div class="form-group">
									<label for="inputPassword" class="col-form-label">Group</label>

									<select id="rows0.variety" name="rows[0].variety"
										class="form-control">
										<option selected="selected" value="1">all</option>
										<option value="2">Linux Servers</option>
										<option value="3">Templates</option>
										<option value="4">Templates/Network devices</option>
										<option value="5">Templates/Operating systems</option>
										<option value="5">Zabbix servers</option>
									</select>

								</div>
							</div>
							<button class="btn blue-btn">Create Host</button>
							<button class="btn blue-btn">Import</button>
						</div>
					</div>
					<!--
                    <div class="bread-crumb">
                        <nav aria-label="breadcrumb">
                          <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="#">All Hosts</a></li>
                            <li class="breadcrumb-item active" aria-current="page">0010</li>
                          </ol>
                        </nav>
                    </div>
-->



				</div>
			</div>
			<aside class="filter-outer">
				<div class="filter-nav row">
					<div class="col-sm-12">
						<ul>
							<li><a href="#"><i class="fa fa-filter"></i> Filter</a></li>
						</ul>
					</div>
				</div>

				<div class="filter-section">
					<div class="">
						<div class="row filter-form-section">
							<div class="col-sm-5">
								<div class="filter-form-inner">
									<div class="form-group row pr-5">
										<label class="col-sm-2 col-form-label">Name</label>
										<div class="col-sm-10">
											<input type="password" class="form-control" id=""
												placeholder="">
										</div>
									</div>
									<div class="form-group row">
										<label class="col-sm-2 col-form-label">Templates</label>
										<div class="col-sm-10">
											<input type="password" class="form-control temp-input" id=""
												placeholder="">
											<button type="button" class="btn select-btn">Select</button>
										</div>
									</div>

									<div class="form-group row pr-5">
										<label class="col-sm-2 col-form-label">DNS</label>
										<div class="col-sm-10">
											<input type="password" class="form-control" id=""
												placeholder="">
										</div>
									</div>
									<div class="form-group row pr-5">
										<label class="col-sm-2 col-form-label">Port</label>
										<div class="col-sm-10">
											<input type="password" class="form-control" id=""
												placeholder="">
										</div>
									</div>
								</div>
							</div>





						</div>
						<div class="row">
							<div class="col-sm-12 text-center mt-2">
								<button type="button" class="btn blue-btn">Apply</button>
								<button type="button" class="btn reset-btn">Reset</button>
							</div>
						</div>



					</div>
				</div>
			</aside>
			<div class="table-outer">
				<div class="custom-table table-responsive">
					<table class="table hosttable">
						<thead>
							<tr>
								<th>
									<div class="">
										<input class="custom-check" id="remember" type="checkbox">
										<label for="remember">&nbsp;</label>
									</div>
								</th>
								<th>Name</th>
								<th>Application</th>
								<th>Items</th>
								<th>Triggers</th>
								<th>Graphs</th>
								<th>Discovery</th>
								<th>Web</th>
								<th>Interface</th>
								<th>Proxy</th>
								<th>Templates</th>
								<th>Status</th>
								<th>Availability</th>
								<th>Agent encryption</th>
								<th>Info</th>
								<th>Tags</th>

							</tr>
						</thead>
						<tbody>





							<tr th:each="host : ${host}">
								<td>
									<div>
										<input class="custom-check" id="check6" type="checkbox">
										<label for="check6">&nbsp;</label>
									</div>
								</td>
								<td th:text="${host.name}">David Right</td>
								<td th:text="${host.application}">Application 1</td>
								<td th:text="${host.items}">Host 250</td>
								<td th:text="${host.triggers}">Triggers 20</td>
								<td th:text="${host.graphs}">Graph 12</td>
								<td th:text="${host.discovery}">Discovery 4</td>
								<td th:text="${host.web}">Web</td>
								<td th:text="${host.interface}">05-26-2020</td>
								<td th:text="${host.proxy}">Lorem Ipsum is simply dummy
									text of the printing and typesetting</td>
								<td th:text="${host.templates}" class="green">Enable</td>
								<td th:text="${host.status}" class="badge-btn"><span
									class="badge badge-light">26X</span> <span
									class="badge badge-success">SNMP</span> <span
									class="badge badge-light">JMX</span></td>

								<td th:text="${host.availability}" class="badge-btn"><span
									class="badge badge-success">NONE</span></td>
								<td th:text="${host.agentencryption}">David Right</td>
								<td th:text="${host.info}">David Right</td>
								<td th:text="${host.tags}">David Right</td>
							</tr>





						</tbody>
					</table>
				</div>
			</div>
		</div>



	</section>


